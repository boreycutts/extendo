{
    "name": "Extendo",
    "description": "Extendo clip for URLs",
    "version": "6.9",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": ["tabs", "storage"],
    "browser_action": {
        "default_popup": "popup.html"
    }
  }